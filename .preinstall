#!usr/bin/env fish

if ! type devd >/dev/null
  printf "Installing devd..."
  if type go
    go get github.com/cortesi/devd/cmd/devd
  else
    fish -c "curl -L https://github.com/cortesi/devd/releases/download/v0.5/devd-0.5-linux64.tgz"
    #TODO Proccess zip file
  end  
end

printf "preinstallation successful\n"